{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "Rastro",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#60A5FA",
    "light": "#3B82F6",
    "dark": "#93C5FD",
    "background": {
      "dark": "#0F0F0F"
    }
  },
  "font": {
    "headings": {
      "family": "Inter",
      "weight": 600
    },
    "body": {
      "family": "Inter",
      "weight": 400
    }
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.rastro.ai"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "API",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart", "authentication"]
          },
          {
            "group": "Enrich",
            "pages": ["enrich/examples", "enrich/overview"]
          },
          {
            "group": "Flows",
            "pages": ["flows/examples", "flows/overview", "flows/execute", "flows/status"]
          },
          {
            "group": "Catalogs",
            "pages": ["catalogs/examples", "catalogs/overview", "catalogs/items", "catalogs/bulk"]
          }
        ]
      }
    ]
  },
  "api": {
    "baseUrl": "https://catalogapi.rastro.ai/api"
  }
}
