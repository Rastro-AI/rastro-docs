{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "Rastro",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#60A5FA",
    "light": "#3B82F6",
    "dark": "#93C5FD"
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.rastro.ai"
  },
  "navigation": {
    "groups": [
      {
        "group": "Getting Started",
        "pages": ["introduction", "quickstart", "authentication"]
      },
      {
        "group": "Enrich",
        "pages": ["enrich/quickstart", "enrich/overview"]
      },
      {
        "group": "Flows",
        "pages": ["flows/quickstart", "flows/execute", "flows/status"]
      },
      {
        "group": "Catalogs",
        "pages": [
          "catalogs/quickstart",
          "catalogs/list",
          "catalogs/create",
          "catalogs/items",
          "catalogs/get-item",
          "catalogs/update-item",
          "catalogs/bulk"
        ]
      }
    ]
  },
  "api": {
    "baseUrl": "https://catalogapi.rastro.ai/api"
  },
  "contextual": {
    "options": ["copy", "chatgpt", "claude"]
  }
}
